-- Top products by total sales

SELECT product_id, SUM(Sales) AS Total_Sales
FROM Amazon_Sales
GROUP BY product_id
ORDER BY Total_Sales DESC;

-- Monthly sales trend

SELECT Month, SUM(Sales) AS Monthly_Sales
FROM Amazon_Sales
GROUP BY Month
ORDER BY Monthly_Sales DESC;

-- Sales by Category

SELECT Category, 
       SUM(Sales) AS Total_Sales
FROM Amazon_Sales
GROUP BY Category;

-- Monthly Sales Trend

SELECT Month, 
       SUM(Sales) AS Monthly_Sales
FROM Amazon_Sales
GROUP BY Month
ORDER BY strftime('%m', month);



